{
    "manifest_version": 2,
   
    "name": "Outlier Desktop",
    "version": "0.2.0",
    "description": "Make outlier desktop not suck too much",
   
    "content_scripts": [{
      "css": ["styles.css"],
      "js": ["content.js"],
      "matches": ["https://outlier.nyc/*"]
    }],
    "icons": {
      "16": "swanbs16.jpg",
      "48": "swanbs.jpg",
      "128": "swanbs128.jpg"
    },
    "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    },
    "permissions": [
      "storage"
    ]
  }